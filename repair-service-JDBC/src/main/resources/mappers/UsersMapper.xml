<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.solvd.university.persistence.StudentContactRepository">
    <insert id="create" keyColumn="student_contact_id" keyProperty="studentContactId" useGeneratedKeys="true">
        INSERT
        INTO student_contacts (phone, email)
        VALUES (#{phone}, #{email})
    </insert>
</mapper>